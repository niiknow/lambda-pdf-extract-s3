    "debug": "^4.1.0",
    "got": "^9.3.2",
    "mkdirp": "^0.5.1",
    "recursive-readdir-sync": "^1.0.6",
    "source-map-support": "^0.5.9",
    "temp": "^0.8.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
