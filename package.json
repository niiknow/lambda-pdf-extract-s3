    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.27.2",
    "jest": "^23.6.0",
    "serverless": "^1.33.2",
    "serverless-offline": "^3.31.3",
    "serverless-plugin-existing-s3": "^2.2.2",
    "serverless-webpack": "^5.2.0",
    "webpack": "^4.26.0",
    "webpack-node-externals": "^1.7.2"
